frontend http_front
        bind *:80
        bind *:443 ssl crt /etc/letsencrypt/live/www.learnengineering.tech/all.pem
        default_backend http_back

backend http_back
        balance roundrobin
        server 511141-web-01 54.227.195.180:80 check
        server 511141-web-02 34.239.255.131:80 check
